* {
  padding 0
  margin 0
}
html {
  font-size 10px
}
html, body, #app {
  width 100%
  height 100%
  overflow auto
}
#app {
  position relative
}
#bg {
  position absolute
  top 0
  left 0
  right 0
  bottom 0
  z-index 0
  background rgba(206, 133, 226, 0.35)
}
@media screen and (max-width 767px) {
  html {
    font-size 8px
  }
}